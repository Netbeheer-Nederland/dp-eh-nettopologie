@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dp_eh_nettopologie: <https://w3id.org/Netbeheer-Nederland/dp-eh-nettopologie/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema1: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schema1:Thing a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic grouping for any identifiable entity" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path schema1:description ],
        [ sh:datatype xsd:anyURI ;
            sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path schema1:identifier ],
        [ sh:datatype xsd:string ;
            sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path schema1:name ] ;
    sh:targetClass schema1:Thing .

dp_eh_nettopologie:IdentifiedObjectCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "A holder for IdentifiedObject objects" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class dp_eh_nettopologie:IdentifiedObject ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path dp_eh_nettopologie:entries ] ;
    sh:targetClass dp_eh_nettopologie:IdentifiedObjectCollection .

dp_eh_nettopologie:IdentifiedObject a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents a IdentifiedObject" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "Number of years since birth" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path dp_eh_nettopologie:age_in_years ],
        [ sh:description "living or dead status" ;
            sh:in ( PATO:0001421 PATO:0001422 "UNKNOWN" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path dp_eh_nettopologie:vital_status ],
        [ sh:datatype xsd:date ;
            sh:description "Date on which a person is born" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path schema1:birthDate ],
        [ sh:datatype xsd:string ;
            sh:description "The main email address of a person" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path schema1:email ;
            sh:pattern "^\\S+@[\\S+\\.]+\\S+" ],
        [ sh:datatype xsd:anyURI ;
            sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path schema1:identifier ],
        [ sh:datatype xsd:string ;
            sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path schema1:name ],
        [ sh:datatype xsd:string ;
            sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path schema1:description ] ;
    sh:targetClass dp_eh_nettopologie:IdentifiedObject .

